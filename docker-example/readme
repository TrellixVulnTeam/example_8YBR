# install docker
yum install docker.io
apt-get install docker.io

# leave container shell
Ctrl+P -> Ctrl+Q

# look up images
docker images

# inspect images
docker inspect [IMAGE ID]

# remove image
docker rmi [IMAGE ID]

# look up running containers
docker ps -a

# attach container
docker attach [CONTAINER ID]

# kill container
docker rm [CONTAINER ID]

# inspect container
docker inspect [CONTAINER ID]

